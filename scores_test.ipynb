   "source": [
    "from py2neo import Graph\n",
    "\n",
    "#Connection to neo4j db\n",
    "SERVER_ADRESSE = \"bolt://localhost:7687\"\n",
    "SERVER_AUTH = (\"neo4j\", \"bioinfo\")\n",
    "graph = Graph(SERVER_ADRESSE, auth=SERVER_AUTH)"
   ]
  },
  {
   "cell_type": "raw",
   "id": "fdb2f898",
   "metadata": {},
   "source": [
    "q = \"match (u:User) return u.name as username\"\n",
    "res = graph.run(q).to_table()\n",
    "for record in res :\n",
    "    print(record[0])"
   ]
  },
